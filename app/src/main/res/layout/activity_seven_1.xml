<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_seven_1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.derrthom.appculture.seven_1"
    android:background="@drawable/aboutme">

    <TextView
        android:text="English"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView6"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        android:textColor="@android:color/background_light" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratingBar5"
        android:rating="4"
        android:layout_below="@+id/textView6"
        android:layout_alignParentEnd="true"
        android:clickable="false"
        android:contextClickable="false" />

    <TextView
        android:text="Hindi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:textSize="24sp"
        android:textColor="@android:color/background_light"
        android:textStyle="normal|bold"
        android:layout_below="@+id/ratingBar5"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratingBar6"
        android:layout_below="@+id/textView7"
        android:layout_alignParentEnd="true"
        android:rating="3"
        android:contextClickable="false"
        android:clickable="false" />

    <TextView
        android:text="Kannada"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView13"
        android:textStyle="normal|bold"
        android:textSize="24sp"
        android:textColor="@android:color/background_light" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView13"
        android:layout_alignStart="@+id/ratingBar6"
        android:id="@+id/ratingBar7"
        android:rating="3.5"
        android:clickable="false"
        android:contextClickable="false" />

    <TextView
        android:text="Malayalam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ratingBar7"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:id="@+id/textView14"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        android:textColor="@android:color/background_light" />

    <RatingBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView14"
        android:layout_alignStart="@+id/ratingBar7"
        android:id="@+id/ratingBar8"
        android:rating="2"
        android:clickable="false"
        android:contextClickable="false" />
</RelativeLayout>
